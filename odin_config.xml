<config>

	<inputs>

		

		

		<input_type>verilog</input_type>

		

		<input_path_and_name>blink.v</input_path_and_name>

	</inputs>

	<output>

		

		<output_type>blif</output_type>

		<output_path_and_name>blink.odin.blif</output_path_and_name>

		<target>

			

			<arch_file>EArch.xml</arch_file>

		</target>

	</output>

	<optimizations>

		<multiply fixed="1" fracture="0" padding="-1" size="3" />

		<memory split_memory_depth="15" split_memory_width="1" />

		<adder size="0" threshold_size="1" />

	</optimizations>

	<debug_outputs>

		

		<debug_output_path>.</debug_output_path>

		<output_ast_graphs>1</output_ast_graphs>

		<output_netlist_graphs>1</output_netlist_graphs>

	</debug_outputs>

</config>